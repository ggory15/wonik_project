<?xml version="1.0"?>
<launch>
	<arg name="sendTF" default="true"/>
	<param if="$(arg sendTF)" name="mecanum_node/sendTransform" value="true"/>
	<param unless="$(arg sendTF)" name="mecanum_node/sendTransform" value="false"/>
	<rosparam command="load" ns="mecanum_node" file="$(find wonik_real)/configs/kinematics/kinematics.yaml"/>
	<node pkg="wonik_kinematics_mecanum" type="wonik_kinematics_mecanum_node" name="mecanum_node" ns="mecanum_node" respawn="false" output="screen">
	</node>
</launch>


