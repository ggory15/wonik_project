<?xml version="1.0"?>
<launch>

 <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <!-- Costmaps -->
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/costmaps/costmap_common.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/costmaps/costmap_common.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/costmaps/costmap_local.yaml" command="load" />
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/costmaps/costmap_global.yaml" command="load" />

    <!-- Global Planner -->

    <!-- NavfnROS -->
    <param name="base_global_planner" value="navfn/NavfnROS"/>
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/global_planner/NavFnROS/navfnros_planner.yaml" command="load" />

    <!-- Local Planner -->

    <!-- EBand -->
    <!-- param name="base_local_planner" value="eband_local_planner/EBandPlannerROS"/-->
    <!-- rosparam file="$(find wonik_simulation)/config/mpo_700/move_base/local_planner/eband_local_planner/eband_planner.yaml" command="load" /-->

    <!-- Teb -->
    <!-- <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS"/>
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/local_planner/teb_local_planner/teb_planner.yaml" command="load" />
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/local_planner/teb_local_planner/costmap_converter_params.yaml" command="load" /> -->

    <!-- WonikLocalPlanner -->
    <param name="base_local_planner" value="wonik_local_planner/WonikLocalPlanner"/> 
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/local_planner/wonik_local_planner/wonik_planner.yaml" command="load" />

    <!-- Common -->
    <rosparam file="$(find wonik_real)/configs/navigation/move_base/move_base/move_base.yaml" command="load" />
    <remap from="cmd_vel"    to="cmd_vel_nav" />
  </node>

</launch>
