<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<!-- upload parameters-->
	<rosparam command="load" ns="wonik_teleop" file="$(find wonik_teleop)/launch/configs/default.yaml"/>

	<!-- start cob teleop node -->
	<node pkg="wonik_teleop" type="wonik_teleop" name="wonik_teleop_node" ns="wonik_teleop" output="screen">

	</node>
	<!-- run joystick -->
  <node pkg="joy" type="joy_node" name="joy_node" output="screen" >
    <param name="dev" type="string" value="/dev/input/js0" />
    <param name="deadzone" value="0.12" />
  </node>
</launch>
